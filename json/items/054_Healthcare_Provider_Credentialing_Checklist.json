    "author": "Jane Doe",
    "status": "In Progress",
    "sections": [
        {
            "sectionId": "SEC-1",
            "sectionName": "Personal Information",
            "description": "This section collects personal information about the healthcare provider.",
            "items": [
                {
                    "itemId": "ITEM-1",
                    "itemName": "Full Name",
                    "description": "The full name of the healthcare provider.",
                    "type": "Document",
                    "required": true
                },
                {
                    "itemId": "ITEM-2",
                    "itemName": "Date of Birth",
                    "description": "The date of birth of the healthcare provider.",
                    "type": "Document",
                    "required": true
                },
                {
                    "itemId": "ITEM-3",
                    "itemName": "Social Security Number",
                    "description": "The social security number of the healthcare provider.",
                    "type": "Document",
                    "required": true
                }
            ]
        },
        {
            "sectionId": "SEC-2",
            "sectionName": "Education and Training",
            "description": "This section collects information about the healthcare provider's education and training.",
            "items": [
                {
                    "itemId": "ITEM-4",
                    "itemName": "Medical School",
                    "description": "The name of the medical school the healthcare provider attended.",
                    "type": "Document",
                    "required": true
                },
                {
                    "itemId": "ITEM-5",
                    "itemName": "Residency",
                    "description": "The name of the residency program the healthcare provider completed.",
                    "type": "Document",
                    "required": true
                },
                {
                    "itemId": "ITEM-6",
                    "itemName": "Fellowship",
                    "description": "The name of the fellowship program the healthcare provider completed.",
                    "type": "Document",
                    "required": false
                }
            ]
        },
        {
            "sectionId": "SEC-3",
            "sectionName": "Licenses and Certifications",
            "description": "This section collects information about the healthcare provider's licenses and certifications.",
            "items": [
                {
                    "itemId": "ITEM-7",
                    "itemName": "Medical License",
                    "description": "The number and state of the healthcare provider's medical license.",
                    "type": "Document",
                    "required": true
                },
                {
                    "itemId": "ITEM-8",
                    "itemName": "DEA Registration",
                    "description": "The number and state of the healthcare provider's DEA registration.",
                    "type": "Document",
                    "required": true
                },
                {
                    "itemId": "ITEM-9",
                    "itemName": "Board Certification",
                    "description": "The name of the board certification the healthcare provider holds.",
                    "type": "Document",
                    "required": false
                }
            ]
        }
    ]
