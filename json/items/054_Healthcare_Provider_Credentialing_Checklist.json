{
    "checklistId": "CL-12345",
    "checklistName": "Healthcare Provider Credentialing Checklist",
    "description": "This checklist is used to ensure that all necessary information is collected from healthcare providers prior to granting them access to the hospital.",
    "createDate": "2023-03-08",
